fdf : client.o manager.o engine.o data.o
	cc -o fdf client.o manager.o engine.o data.o

client.o : client/client.c report/report.c client/client.h
	cc -c client/client.c report/report.c
manager.o : manager/manager.c manager/manager.h
	cc -c manager/manager.c
engine.o : engine/engine.c engine/engine.h
	cc -c engine/engine.c
data.o : data/data.c data/data.h
	cc -c data/data.c
clean :
	rm fdf client.o manager.o engine.o data.o

